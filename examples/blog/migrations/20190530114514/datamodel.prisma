model Blog {
  id: Int @id
  name: String
  viewCount: Int
  posts: Post[]
  authors: Author[]
}

model Author {
  id: Int @id
  name: String?
  authors: Blog[]
}         

model Post {
  id: Int @id
  title2: String
  anotherText: String
  tags: String[]
  blog: Blog
} 

model AnotherModel {
  id: Int @id
  title: String
  anotherText: String
  anotherText2: String
  anotherText3: String
  tags: String[]
}

